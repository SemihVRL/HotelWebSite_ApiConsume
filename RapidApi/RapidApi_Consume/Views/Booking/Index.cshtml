@model List<BookingApiViewModel.Result>
@{
    Layout = null;
    int count = 0;
}



<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">


    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Otel Adı</th>
            <th>puanı</th>
            <th>detaylar</th>

        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@count</td>
                <td>@x.name</td>
                <td>@x.reviewScore</td>
                <td><a href="#" class="btn btn-success">Güncelle</a></td>

            </tr>
        }


    </table>

</div>